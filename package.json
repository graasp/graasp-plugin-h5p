{
  "name": "graasp-plugin-h5p",
  "author": "Graasp Association",
  "version": "0.1.0",
  "description": "Graasp back-end support for H5P as a Fastify plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "pre-commit": "pretty-quick --staged && yarn lint",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest  --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "lint": "eslint ."
  },
  "repository": "git@github.com:graasp/graasp-plugin-h5p.git",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/graasp/graasp-plugin-h5p/issues"
  },
  "homepage": "https://github.com/graasp/graasp-plugin-h5p#readme",
  "dependencies": {
    "@fastify/multipart": "6.0.0",
    "@fastify/static": "5.0.2",
    "ajv": "8.11.0",
    "extract-zip": "2.0.1",
    "fastify-plugin": "3.0.1",
    "graasp-plugin-file": "github:graasp/graasp-plugin-file",
    "graasp-plugin-file-item": "github:graasp/graasp-plugin-file-item",
    "h5p-standalone": "3.5.1",
    "http-status-codes": "2.2.0",
    "mime": "3.0.0",
    "secure-json-parse": "2.4.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/extract-zip": "2.0.1",
    "@types/graasp": "github:graasp/graasp-types",
    "@types/jest": "27.0.1",
    "@types/mime": "2.0.3",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.3.0",
    "fastify": "3.29.0",
    "husky": "7.0.4",
    "jest": "27.3.0",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.2",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  },
  "packageManager": "yarn@3.2.0-rc.8"
}
